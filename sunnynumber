import java.io.*;
import java.util.Scanner;
import java.lang.Math;
class HelloWorld {
    public static void main(String[] args) {
        int n;
        double square;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        square=Math.sqrt(n+1);
        int intSqrt=(int)square;
        if (intSqrt * intSqrt == n + 1) 
        System.out.print("yes");
        else
        System.out.print("No");
    }
}
